@{
}


@foreach (var item in this.User.Claims)
{
    <p>@item.Type : @item.Value</p>
}

@if (User.Identity.IsAuthenticated)
{
    <p> access token: @ViewBag.accessToken </p>
    <p>id token : @ViewBag.idToken</p>
}

<a href="/Account/Signout">Click to Logout</a>
